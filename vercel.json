{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm run build --filter=@ticket-scheduler/next",
  "outputDirectory": "apps/next/.next",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "crons": [
    {
      "path": "/api/push/schedule",
      "schedule": "* * * * *"
    }
  ]
}
