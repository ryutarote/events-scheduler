{
  "framework": "nextjs",
  "installCommand": "npm install -g pnpm && cd ../.. && pnpm install",
  "buildCommand": "cd ../.. && pnpm run build --filter=@ticket-scheduler/next",
  "outputDirectory": ".next",
  "crons": [
    {
      "path": "/api/push/schedule",
      "schedule": "* * * * *"
    }
  ]
}
